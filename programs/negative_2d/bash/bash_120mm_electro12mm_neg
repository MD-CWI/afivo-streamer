./streamer air_negative_120mm_chem_electro12mm.cfg -field_amplitude=1.045e6 -field_decay%enabled=F -cross%write=T -output%name+=bd1e11_EL12mm_ER1.2mm_neg/neg_bd1e11_12mm_1.2mm_1.045e6

./streamer air_negative_120mm_chem_electro12mm.cfg -field_amplitude=1.04e6 -field_decay%enabled=F -cross%write=T -output%name+=bd1e11_EL12mm_ER1.2mm_neg/neg_bd1e11_12mm_1.2mm_1.04e6

./streamer air_negative_120mm_chem_electro12mm.cfg -field_amplitude=1.03e6 -field_decay%enabled=F -cross%write=T -output%name+=bd1e11_EL12mm_ER1.2mm_neg/neg_bd1e11_12mm_1.2mm_1.03e6

./streamer air_negative_120mm_chem_electro12mm.cfg -field_amplitude=1.01e6 -field_decay%enabled=F -cross%write=T -output%name+=bd1e11_EL12mm_ER1.2mm_neg/neg_bd1e11_12mm_1.2mm_1.01e6

./streamer air_negative_120mm_chem_electro12mm.cfg -field_amplitude=0.98e6 -field_decay%enabled=F -cross%write=T -output%name+=bd1e11_EL12mm_ER1.2mm_neg/neg_bd1e11_12mm_1.2mm_0.98e6

./streamer air_negative_120mm_chem_electro12mm.cfg -field_amplitude=0.94e6 -field_decay%enabled=F -cross%write=T -output%name+=bd1e11_EL12mm_ER1.2mm_neg/neg_bd1e11_12mm_1.2mm_0.94e6

./streamer air_negative_120mm_chem_electro12mm.cfg -field_amplitude=0.89e6 -field_decay%enabled=F -cross%write=T -output%name+=bd1e11_EL12mm_ER1.2mm_neg/neg_bd1e11_12mm_1.2mm_0.89e6

