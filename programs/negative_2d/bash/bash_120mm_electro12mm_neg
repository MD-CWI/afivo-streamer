./streamer air_negative_120mm_chem_electro12mm.cfg -field_amplitude=0.8e6 -field_decay%enabled=F -cross%write=T -output%name+=bd1e11_EL12mm_ER1.2mm_neg/neg_bd1e11_12mm_1.2mm_0.8e6

./streamer air_negative_120mm_chem_electro12mm.cfg -field_amplitude=0.7e6 -field_decay%enabled=F -cross%write=T -output%name+=bd1e11_EL12mm_ER1.2mm_neg/neg_bd1e11_12mm_1.2mm_0.7e6

