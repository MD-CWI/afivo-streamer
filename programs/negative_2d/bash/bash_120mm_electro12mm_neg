#!/bin/bash

cd ../

# air, bd=1e11, electrode_12mm_1.2mm, refine_min_dx=1e-6, refine_adx=1, refine_adx_fac=1.25, air_phelps_chemistry_small_v0_1000points.txt

# the fading out of negative streamers under constant electric field (long electrode)
./streamer air_negative_120mm_chem_electro12mm.cfg -field_amplitude=1.6e6 -field_decay%enabled=F -cross%write=T -output%name+=bd1e11_EL12mm_ER1.2mm_neg/neg_bd1e11_12mm_1.2mm_1.6e6

./streamer air_negative_120mm_chem_electro12mm.cfg -field_amplitude=1.5e6 -field_decay%enabled=F -cross%write=T -output%name+=bd1e11_EL12mm_ER1.2mm_neg/neg_bd1e11_12mm_1.2mm_1.5e6

