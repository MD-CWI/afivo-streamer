#!/bin/bash

cd ../

./streamer air_negative_v160mm.cfg -field_decay%min_field=1.3e6 -field_decay%decay_distance=5e-3 -refine_min_dx=5e-6 -refine_adx=1 -refine_adx_fac=1 -dt_min=1e-16 -output%name+=exponential/min_field/chemistry_style/sd5e19_2.1e6_5e-3_145e-3/50points_runtime/1.3e6_oldstyle_mindx5e-6_adx1_fac1_dtmin1e-16

