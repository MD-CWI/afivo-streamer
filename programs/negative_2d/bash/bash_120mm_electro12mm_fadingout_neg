./streamer air_negative_120mm_chem_electro12mm.cfg -field_amplitude=1.02884e6 -field_decay%enabled=F -cross%write=T -lineout%write=T -output%name+=bd1e11_EL12mm_ER1.2mm_fadingout_neg/neg_bd1e11_EL12mm_1.156e6

