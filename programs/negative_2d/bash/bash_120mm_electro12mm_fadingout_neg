./streamer air_negative_120mm_chem_electro12mm.cfg -field_amplitude=1.01015e6 -field_decay%enabled=F -cross%write=T -lineout%write=T -output%name+=bd1e11_EL12mm_ER1.2mm_fadingout_neg/neg_bd1e11_EL12mm_1.135e6

./streamer air_negative_120mm_chem_electro12mm.cfg -field_amplitude=0.99502e6 -field_decay%enabled=F -cross%write=T -lineout%write=T -output%name+=bd1e11_EL12mm_ER1.2mm_fadingout_neg/neg_bd1e11_EL12mm_1.118e6

./streamer air_negative_120mm_chem_electro12mm.cfg -field_amplitude=0.97455e6 -field_decay%enabled=F -cross%write=T -lineout%write=T -output%name+=bd1e11_EL12mm_ER1.2mm_fadingout_neg/neg_bd1e11_EL12mm_1.095e6

./streamer air_negative_120mm_chem_electro12mm.cfg -field_amplitude=0.94251e6 -field_decay%enabled=F -cross%write=T -lineout%write=T -output%name+=bd1e11_EL12mm_ER1.2mm_fadingout_neg/neg_bd1e11_EL12mm_1.059e6

./streamer air_negative_120mm_chem_electro12mm.cfg -field_amplitude=0.8811e6 -field_decay%enabled=F -cross%write=T -lineout%write=T -output%name+=bd1e11_EL12mm_ER1.2mm_fadingout_neg/neg_bd1e11_EL12mm_0.99e6

