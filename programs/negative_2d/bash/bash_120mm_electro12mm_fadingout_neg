#!/bin/bash

cd ../

# air, background density=1e11, electrode length=12e-3, electode radius=1.2e-3, refine_min_dx=1e-6, refine_adx=0.8, refine_adx_fac=1.25, air_phelps_chemistry_small_v0_1000points.txt


# the fading out of negative streamers under constant electric field with long electrode


./streamer air_negative_120mm_chem_electro12mm.cfg -field_amplitude=1.0858e6 -field_decay%enabled=F -cross%write=T -lineout%write=T -output%name+=bd1e11_EL12mm_ER1.2mm_fadingout_neg/neg_bd1e11_EL12mm_1.22e6

./streamer air_negative_120mm_chem_electro12mm.cfg -field_amplitude=1.0591e6 -field_decay%enabled=F -cross%write=T -lineout%write=T -output%name+=bd1e11_EL12mm_ER1.2mm_fadingout_neg/neg_bd1e11_EL12mm_1.19e6

