./streamer air_d40mm_chemistry.cfg -field_waveform%pulse_field=-1.6e6 -field_waveform%turn_off_z=10e-3 -field_rod_r0="0 0.91" -field_rod_radius=0.4e-3 -end_streamer_length=3.5e-2 -output%name=/export/scratch2/baohong/chemistry/d40mm_double_pulses/EL4mm_SL26mm_1.6e6

./streamer air_d40mm_chemistry.cfg -field_waveform%pulse_field=-1.7e6 -field_waveform%turn_off_z=10e-3 -field_rod_r0="0 0.91" -field_rod_radius=0.4e-3 -end_streamer_length=3.5e-2 -output%name=/export/scratch2/baohong/chemistry/d40mm_double_pulses/EL4mm_SL26mm_1.7e6

./streamer air_d40mm_chemistry.cfg -field_waveform%pulse_field=-1.8e6 -field_waveform%turn_off_z=10e-3 -field_rod_r0="0 0.91" -field_rod_radius=0.4e-3 -end_streamer_length=3.5e-2 -output%name=/export/scratch2/baohong/chemistry/d40mm_double_pulses/EL4mm_SL26mm_1.8e6

./streamer air_d40mm_chemistry.cfg -field_waveform%pulse_field=-2.0e6 -field_waveform%turn_off_z=10e-3 -field_rod_r0="0 0.91" -field_rod_radius=0.4e-3 -end_streamer_length=3.5e-2 -output%name=/export/scratch2/baohong/chemistry/d40mm_double_pulses/EL4mm_SL26mm_2.0e6

