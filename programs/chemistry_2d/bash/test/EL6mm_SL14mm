#!/bin/bash

cd ../

./streamer air_d40mm_chemistry.cfg -field_waveform%pulse_field=-0.9e6 -field_waveform%turn_off_z=20e-3 -field_rod_r0="0 0.865" -field_rod_radius=0.6e-3 -end_streamer_length=3.3e-2 -output%name+=EL6mm_SL14mm_0.9e6

./streamer air_d40mm_chemistry.cfg -field_waveform%pulse_field=-1.0e6 -field_waveform%turn_off_z=20e-3 -field_rod_r0="0 0.865" -field_rod_radius=0.6e-3 -end_streamer_length=3.3e-2 -output%name+=EL6mm_SL14mm_1.0e6

