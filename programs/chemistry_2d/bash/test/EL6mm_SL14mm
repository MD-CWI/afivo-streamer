./streamer air_d40mm_chemistry.cfg -field_waveform%pulse_field=-1.4e6 -field_waveform%turn_off_z=20e-3 -field_rod_r0="0 0.865" -field_rod_radius=0.6e-3 -end_streamer_length=3.3e-2 -output%name+=EL6mm_SL14mm_1.4e6

./streamer air_d40mm_chemistry.cfg -field_waveform%pulse_field=-1.5e6 -field_waveform%turn_off_z=20e-3 -field_rod_r0="0 0.865" -field_rod_radius=0.6e-3 -end_streamer_length=3.3e-2 -output%name+=EL6mm_SL14mm_1.5e6

./streamer air_d40mm_chemistry.cfg -field_waveform%pulse_field=-1.6e6 -field_waveform%turn_off_z=20e-3 -field_rod_r0="0 0.865" -field_rod_radius=0.6e-3 -end_streamer_length=3.3e-2 -output%name+=EL6mm_SL14mm_1.6e6

./streamer air_d40mm_chemistry.cfg -field_waveform%pulse_field=-1.8e6 -field_waveform%turn_off_z=20e-3 -field_rod_r0="0 0.865" -field_rod_radius=0.6e-3 -end_streamer_length=3.3e-2 -output%name+=EL6mm_SL14mm_1.8e6

./streamer air_d40mm_chemistry.cfg -field_waveform%pulse_field=-2.0e6 -field_waveform%turn_off_z=20e-3 -field_rod_r0="0 0.865" -field_rod_radius=0.6e-3 -end_streamer_length=3.3e-2 -output%name+=EL6mm_SL14mm_2.0e6

./streamer air_d40mm_chemistry.cfg -field_waveform%pulse_field=-2.2e6 -field_waveform%turn_off_z=20e-3 -field_rod_r0="0 0.865" -field_rod_radius=0.6e-3 -end_streamer_length=3.3e-2 -output%name+=EL6mm_SL14mm_2.2e6

./streamer air_d40mm_chemistry.cfg -field_waveform%pulse_field=-2.4e6 -field_waveform%turn_off_z=20e-3 -field_rod_r0="0 0.865" -field_rod_radius=0.6e-3 -end_streamer_length=3.3e-2 -output%name+=EL6mm_SL14mm_2.4e6

./streamer air_d40mm_chemistry.cfg -field_waveform%pulse_field=-2.6e6 -field_waveform%turn_off_z=20e-3 -field_rod_r0="0 0.865" -field_rod_radius=0.6e-3 -end_streamer_length=3.3e-2 -output%name+=EL6mm_SL14mm_2.6e6

