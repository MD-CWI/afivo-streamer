./streamer air_d20mm_chemistry.cfg -field_waveform%enabled=T -field_waveform%pulse_field=-1.5e6 -field_rod_r0="0 0.91" -field_rod_radius=0.2e-3 -input_data%file=../../transport_data/N2_O2/Air_Kawa_Itikawa_Kossy_modified.txt -output%name=/export/scratch2/md/baohong/chemistry/two_pulse_new/EL2mm_1.5e6_Andy_300ns

./streamer air_d20mm_chemistry.cfg -field_waveform%enabled=T -field_waveform%pulse_field=-2.2e6 -field_rod_r0="0 0.91" -field_rod_radius=0.2e-3 -input_data%file=../../transport_data/N2_O2/air_chemistry_Baohong_reactions.txt -output%name+=EL2mm_2.2e6_new

./streamer air_d20mm_chemistry.cfg -field_waveform%enabled=T -field_waveform%pulse_field=-0.9e6 -field_rod_r0="0 0.82" -field_rod_radius=0.4e-3 -end_streamer_length=1.5e-2 -input_data%file=../../transport_data/N2_O2/air_chemistry_Baohong_reactions.txt -output%name+=EL4mm_0.9e6_conductivity_new

